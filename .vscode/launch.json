  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'panoply'",
      "program": "${workspaceRoot}/target/debug/panoply",
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "LD_LIBRARY_PATH": "${workspaceFolder}/target/debug/deps:${userHome}/.rustup/toolchains/stable-aarch64-apple-darwin/lib"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'panoply' (cargo)",
      "cargo": {
        "args": [
          "build",
          "--bin=panoply",
          "--package=panoply"
        ],
        "filter": {
          "name": "panoply",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "LD_LIBRARY_PATH": "${workspaceFolder}/target/debug/deps:${userHome}/.rustup/toolchains/stable-aarch64-apple-darwin/lib"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'panoply'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=panoply",
          "--package=panoply"
        ],
        "filter": {
          "name": "panoply",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}